    </bpmn:boundaryEvent>
    <bpmn:userTask id="contactCustomer" name="Contact Customer" />
    <bpmn:endEvent id="paymentSuccess" name="Payment Success" />
    <bpmn:endEvent id="paymentFailed" name="Payment Failed" />

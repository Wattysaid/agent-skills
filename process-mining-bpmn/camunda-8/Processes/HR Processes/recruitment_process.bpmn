        <di:waypoint x="915" y="175" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Edge_Flow_Join_to_Decision" bpmnElement="Flow_Join_to_Decision">
        <di:waypoint x="940" y="150" />

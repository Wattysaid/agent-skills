      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Edge_Flow_Split_to_TechInterview" bpmnElement="Flow_Split_to_TechInterview">
        <di:waypoint x="665" y="175" />
        <di:waypoint x="665" y="350" />

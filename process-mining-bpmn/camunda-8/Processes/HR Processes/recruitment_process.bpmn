        <di:waypoint x="1155" y="350" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Edge_Flow_Gateway_Yes" bpmnElement="Flow_Gateway_Yes">
        <di:waypoint x="1180" y="325" />
        <di:waypoint x="1180" y="113" />
        <di:waypoint x="1230" y="113" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1154" y="201" width="18" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Edge_Flow_Gateway_No" bpmnElement="Flow_Gateway_No">
        <di:waypoint x="1205" y="350" />
        <di:waypoint x="1292" y="350" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1213" y="328" width="15" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Edge_Flow_Offer_to_EndHired" bpmnElement="Flow_Offer_to_EndHired">
        <di:waypoint x="1350" y="120" />
        <di:waypoint x="1390" y="120" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>

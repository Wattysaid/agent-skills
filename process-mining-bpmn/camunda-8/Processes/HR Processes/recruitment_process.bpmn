      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Edge_Flow_HRInterview_to_Join" bpmnElement="Flow_HRInterview_to_Join">
        <di:waypoint x="850" y="150" />
        <di:waypoint x="890" y="150" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Edge_Flow_TechInterview_to_Join" bpmnElement="Flow_TechInterview_to_Join">
        <di:waypoint x="850" y="350" />

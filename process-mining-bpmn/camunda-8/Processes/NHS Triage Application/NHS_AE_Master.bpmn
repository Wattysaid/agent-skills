    <bpmn:sequenceFlow id="Flow_ArrivalToRegister" sourceRef="Task_PatientArrives" targetRef="Task_Register" />
    <bpmn:sequenceFlow id="Flow_RegisterToTriage" sourceRef="Task_Register" targetRef="Task_InitialTriage" />
    <bpmn:sequenceFlow id="Flow_TriageToGateway" sourceRef="Task_InitialTriage" targetRef="Gateway_StreamingDecision" />
    <bpmn:sequenceFlow id="Flow_GatewayToMerge" name="Majors/minors/paeds" sourceRef="Gateway_StreamingDecision" targetRef="Gateway_StreamMerge" />
    <bpmn:sequenceFlow id="Flow_EarlyCareToMerge" sourceRef="Task_EarlyTreatment" targetRef="Gateway_StreamMerge" />
    <bpmn:sequenceFlow id="Flow_MergeToDiagnostics" sourceRef="Gateway_StreamMerge" targetRef="Task_Diagnostics" />

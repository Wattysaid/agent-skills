        <bpmn:flowNodeRef>Task_EarlyTreatment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Diagnostics</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_StreamingDecision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Disposition</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AdmitOrTransfer</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Discharge</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_DispositionMerge</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Exit</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:userTask id="Task_PatientArrives" name="Request help / present to A&#38;E" />

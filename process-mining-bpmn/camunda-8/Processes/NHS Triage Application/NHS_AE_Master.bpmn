    <bpmn:userTask id="Task_Register" name="Register and capture demographics" />
    <bpmn:userTask id="Task_InitialTriage" name="Initial triage and acuity" />
    <bpmn:exclusiveGateway id="Gateway_StreamMerge" name="Streaming paths merge" />
    <bpmn:userTask id="Task_EarlyTreatment" name="Immediate care (pain relief, resus if needed)" />
    <bpmn:userTask id="Task_Diagnostics" name="Diagnostics and focused treatment">
      <bpmn:outgoing>Flow_0n77l8n</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_StreamingDecision" name="Where should patient be streamed?" />

        <bpmn:flowNodeRef>Task_Register</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Triage" name="Triage">
        <bpmn:flowNodeRef>Task_InitialTriage</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Clinical" name="Clinical Care &#38; Diagnostics">

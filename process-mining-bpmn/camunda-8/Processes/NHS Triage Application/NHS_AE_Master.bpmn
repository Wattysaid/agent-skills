    <bpmn:sequenceFlow id="Flow_0n77l8n" sourceRef="Task_Diagnostics" targetRef="Gateway_Disposition" />
    <bpmn:sequenceFlow id="Flow_DischargeToPatient" sourceRef="Task_Discharge" targetRef="Task_ReceiveDischargeInfo" />
    <bpmn:sequenceFlow id="Flow_PatientToMerge" sourceRef="Task_ReceiveDischargeInfo" targetRef="Gateway_DispositionMerge" />
    <bpmn:sequenceFlow id="Flow_DispositionToAdmission" name="Admit/transfer" sourceRef="Gateway_Disposition" targetRef="Task_AdmitOrTransfer" />
    <bpmn:sequenceFlow id="Flow_AdmitToMerge" sourceRef="Task_AdmitOrTransfer" targetRef="Gateway_DispositionMerge" />
    <bpmn:exclusiveGateway id="Gateway_Disposition" name="Is disposition decision ready?">
      <bpmn:incoming>Flow_0n77l8n</bpmn:incoming>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_DispositionToDischarge" name="Safe to discharge" sourceRef="Gateway_Disposition" targetRef="Task_Discharge" />
    <bpmn:sequenceFlow id="Flow_1j6lkx2" sourceRef="Gateway_DispositionMerge" targetRef="EndEvent_Exit" />
    <bpmn:userTask id="Task_ReceiveDischargeInfo" name="Receive instructions / transport" />
    <bpmn:userTask id="Task_AdmitOrTransfer" name="Arrange admission or transfer" />
    <bpmn:userTask id="Task_Discharge" name="Prepare discharge &#38; safety netting" />
    <bpmn:exclusiveGateway id="Gateway_DispositionMerge" name="Paths merge before exit">
      <bpmn:outgoing>Flow_1j6lkx2</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="EndEvent_Exit" name="Exit A&#38;E">
      <bpmn:incoming>Flow_1j6lkx2</bpmn:incoming>
    </bpmn:endEvent>

<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_BidApp" targetNamespace="http://example.com/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.40.1">
  <bpmn:process id="Process_BidApp" name="Bid Automation Core Processes" isExecutable="false">
    <bpmn:laneSet id="LaneSet_BidApp">
      <bpmn:lane id="Lane_Systems" name="Platform, CRM &#38; Notifications">
        <bpmn:flowNodeRef>Task_RecordMeeting</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_UpdateDashboard</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_VersionControl</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_FollowUpFlags</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_IntegrateMessaging</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AutoNotifications</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CRMUpdate</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AttachProposalCRM</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GenerateReports</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_RiskDelayDecision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_RiskDelayMerge</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_BidReady</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Governance" name="Finance, Legal &#38; Compliance">
        <bpmn:flowNodeRef>Task_RetrievePricing</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ComplianceChecks</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Operations" name="Operations &#38; Delivery">
        <bpmn:flowNodeRef>Task_CheckInventory</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_BuildProjectTeam</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_AIEngine" name="AI Multi-Agent Engine">
        <bpmn:flowNodeRef>Task_ExtractInsights</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SpeechToText</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CustomizeProposal</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SuggestValueAdds</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RecordMinutes</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SuggestProducts</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_BidOwner" name="Bid Owner / Proposal Manager">
        <bpmn:flowNodeRef>Task_PopulateTemplate</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CentralizeDocs</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AssignTasks</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ScheduleTeamMeetings</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_EscalateDelays</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>StartEvent_BidInitiated</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_BidInitiated" name="Bid initiated">
      <bpmn:outgoing>Flow_Start_to_RecordMeeting</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="Task_RecordMeeting" name="Record meeting (audio/video)">
      <bpmn:incoming>Flow_Start_to_RecordMeeting</bpmn:incoming>
      <bpmn:outgoing>Flow_RecordMeeting_to_SpeechToText</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_SpeechToText" name="Speech-to-text transcription">
      <bpmn:incoming>Flow_RecordMeeting_to_SpeechToText</bpmn:incoming>
      <bpmn:outgoing>Flow_SpeechToText_to_ExtractInsights</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_ExtractInsights" name="Extract action items and pain points">
      <bpmn:incoming>Flow_SpeechToText_to_ExtractInsights</bpmn:incoming>
      <bpmn:outgoing>Flow_ExtractInsights_to_PopulateTemplate</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Task_PopulateTemplate" name="Populate proposal template">
      <bpmn:incoming>Flow_ExtractInsights_to_PopulateTemplate</bpmn:incoming>
      <bpmn:outgoing>Flow_PopulateTemplate_to_CustomizeProposal</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_CustomizeProposal" name="Customize proposal via keyword analysis">
      <bpmn:incoming>Flow_PopulateTemplate_to_CustomizeProposal</bpmn:incoming>
      <bpmn:outgoing>Flow_CustomizeProposal_to_SuggestProducts</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_SuggestProducts" name="Suggest products/services">
      <bpmn:incoming>Flow_CustomizeProposal_to_SuggestProducts</bpmn:incoming>
      <bpmn:outgoing>Flow_SuggestProducts_to_BuildProjectTeam</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Task_BuildProjectTeam" name="Build project team from resources">
      <bpmn:incoming>Flow_SuggestProducts_to_BuildProjectTeam</bpmn:incoming>
      <bpmn:outgoing>Flow_BuildProjectTeam_to_CheckInventory</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_CheckInventory" name="Check inventory and service windows">
      <bpmn:incoming>Flow_BuildProjectTeam_to_CheckInventory</bpmn:incoming>
      <bpmn:outgoing>Flow_CheckInventory_to_RetrievePricing</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_RetrievePricing" name="Retrieve &#38; optimize pricing (catalog + discounts)">
      <bpmn:incoming>Flow_CheckInventory_to_RetrievePricing</bpmn:incoming>
      <bpmn:outgoing>Flow_RetrievePricing_to_SuggestValueAdds</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_SuggestValueAdds" name="Suggest value-adds and upsells">
      <bpmn:incoming>Flow_RetrievePricing_to_SuggestValueAdds</bpmn:incoming>
      <bpmn:outgoing>Flow_SuggestValueAdds_to_ComplianceChecks</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_ComplianceChecks" name="Compliance checks (legal/financial/technical)">
      <bpmn:incoming>Flow_SuggestValueAdds_to_ComplianceChecks</bpmn:incoming>
      <bpmn:outgoing>Flow_ComplianceChecks_to_CentralizeDocs</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Task_CentralizeDocs" name="Centralize contracts, NDAs, specs">
      <bpmn:incoming>Flow_ComplianceChecks_to_CentralizeDocs</bpmn:incoming>
      <bpmn:outgoing>Flow_CentralizeDocs_to_VersionControl</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_VersionControl" name="Maintain proposal versions with audit trail">
      <bpmn:incoming>Flow_CentralizeDocs_to_VersionControl</bpmn:incoming>
      <bpmn:outgoing>Flow_VersionControl_to_UpdateDashboard</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_UpdateDashboard" name="Update bid dashboard (Gantt/timeline)">
      <bpmn:incoming>Flow_VersionControl_to_UpdateDashboard</bpmn:incoming>
      <bpmn:outgoing>Flow_UpdateDashboard_to_AssignTasks</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Task_AssignTasks" name="Assign tasks and track completion">
      <bpmn:incoming>Flow_UpdateDashboard_to_AssignTasks</bpmn:incoming>
      <bpmn:outgoing>Flow_AssignTasks_to_ScheduleTeamMeetings</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_ScheduleTeamMeetings" name="Schedule team meetings/calls">
      <bpmn:incoming>Flow_AssignTasks_to_ScheduleTeamMeetings</bpmn:incoming>
      <bpmn:outgoing>Flow_ScheduleTeamMeetings_to_RecordMinutes</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_RecordMinutes" name="Record minutes and transcript">
      <bpmn:incoming>Flow_ScheduleTeamMeetings_to_RecordMinutes</bpmn:incoming>
      <bpmn:outgoing>Flow_RecordMinutes_to_FollowUpFlags</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_FollowUpFlags" name="Flag required follow-ups">
      <bpmn:incoming>Flow_RecordMinutes_to_FollowUpFlags</bpmn:incoming>
      <bpmn:outgoing>Flow_FollowUpFlags_to_IntegrateMessaging</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_IntegrateMessaging" name="Integrate instant messaging">
      <bpmn:incoming>Flow_FollowUpFlags_to_IntegrateMessaging</bpmn:incoming>
      <bpmn:outgoing>Flow_IntegrateMessaging_to_AutoNotifications</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_AutoNotifications" name="Notify stakeholders and status updates">
      <bpmn:incoming>Flow_IntegrateMessaging_to_AutoNotifications</bpmn:incoming>
      <bpmn:outgoing>Flow_AutoNotifications_to_RiskDelayDecision</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_RiskDelayDecision" name="Delay or risk detected?" gatewayDirection="Diverging">
      <bpmn:incoming>Flow_AutoNotifications_to_RiskDelayDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_RiskDecision_Yes_to_Escalate</bpmn:outgoing>
      <bpmn:outgoing>Flow_RiskDecision_No_to_Merge</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_EscalateDelays" name="Escalate delays or risks">
      <bpmn:incoming>Flow_RiskDecision_Yes_to_Escalate</bpmn:incoming>
      <bpmn:outgoing>Flow_Escalate_to_Merge</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_RiskDelayMerge" name="Risk handling complete" gatewayDirection="Converging">
      <bpmn:incoming>Flow_Escalate_to_Merge</bpmn:incoming>
      <bpmn:incoming>Flow_RiskDecision_No_to_Merge</bpmn:incoming>
      <bpmn:outgoing>Flow_Merge_to_CRMUpdate</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_CRMUpdate" name="Sync CRM with client profile and interactions">
      <bpmn:incoming>Flow_Merge_to_CRMUpdate</bpmn:incoming>
      <bpmn:outgoing>Flow_CRMUpdate_to_AttachProposal</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_AttachProposalCRM" name="Attach proposal and bid documents to CRM">
      <bpmn:incoming>Flow_CRMUpdate_to_AttachProposal</bpmn:incoming>
      <bpmn:outgoing>Flow_AttachProposal_to_GenerateReports</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_GenerateReports" name="Generate periodic bid reports">
      <bpmn:incoming>Flow_AttachProposal_to_GenerateReports</bpmn:incoming>
      <bpmn:outgoing>Flow_GenerateReports_to_End</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_BidReady" name="End - Bid packaged">
      <bpmn:incoming>Flow_GenerateReports_to_End</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_Start_to_RecordMeeting" sourceRef="StartEvent_BidInitiated" targetRef="Task_RecordMeeting" />
    <bpmn:sequenceFlow id="Flow_RecordMeeting_to_SpeechToText" sourceRef="Task_RecordMeeting" targetRef="Task_SpeechToText" />
    <bpmn:sequenceFlow id="Flow_SpeechToText_to_ExtractInsights" sourceRef="Task_SpeechToText" targetRef="Task_ExtractInsights" />
    <bpmn:sequenceFlow id="Flow_ExtractInsights_to_PopulateTemplate" sourceRef="Task_ExtractInsights" targetRef="Task_PopulateTemplate" />
    <bpmn:sequenceFlow id="Flow_PopulateTemplate_to_CustomizeProposal" sourceRef="Task_PopulateTemplate" targetRef="Task_CustomizeProposal" />
    <bpmn:sequenceFlow id="Flow_CustomizeProposal_to_SuggestProducts" sourceRef="Task_CustomizeProposal" targetRef="Task_SuggestProducts" />
    <bpmn:sequenceFlow id="Flow_SuggestProducts_to_BuildProjectTeam" sourceRef="Task_SuggestProducts" targetRef="Task_BuildProjectTeam" />
    <bpmn:sequenceFlow id="Flow_BuildProjectTeam_to_CheckInventory" sourceRef="Task_BuildProjectTeam" targetRef="Task_CheckInventory" />
    <bpmn:sequenceFlow id="Flow_CheckInventory_to_RetrievePricing" sourceRef="Task_CheckInventory" targetRef="Task_RetrievePricing" />
    <bpmn:sequenceFlow id="Flow_RetrievePricing_to_SuggestValueAdds" sourceRef="Task_RetrievePricing" targetRef="Task_SuggestValueAdds" />
    <bpmn:sequenceFlow id="Flow_SuggestValueAdds_to_ComplianceChecks" sourceRef="Task_SuggestValueAdds" targetRef="Task_ComplianceChecks" />
    <bpmn:sequenceFlow id="Flow_ComplianceChecks_to_CentralizeDocs" sourceRef="Task_ComplianceChecks" targetRef="Task_CentralizeDocs" />
    <bpmn:sequenceFlow id="Flow_CentralizeDocs_to_VersionControl" sourceRef="Task_CentralizeDocs" targetRef="Task_VersionControl" />
    <bpmn:sequenceFlow id="Flow_VersionControl_to_UpdateDashboard" sourceRef="Task_VersionControl" targetRef="Task_UpdateDashboard" />
    <bpmn:sequenceFlow id="Flow_UpdateDashboard_to_AssignTasks" sourceRef="Task_UpdateDashboard" targetRef="Task_AssignTasks" />
    <bpmn:sequenceFlow id="Flow_AssignTasks_to_ScheduleTeamMeetings" sourceRef="Task_AssignTasks" targetRef="Task_ScheduleTeamMeetings" />
    <bpmn:sequenceFlow id="Flow_ScheduleTeamMeetings_to_RecordMinutes" sourceRef="Task_ScheduleTeamMeetings" targetRef="Task_RecordMinutes" />
    <bpmn:sequenceFlow id="Flow_RecordMinutes_to_FollowUpFlags" sourceRef="Task_RecordMinutes" targetRef="Task_FollowUpFlags" />
    <bpmn:sequenceFlow id="Flow_FollowUpFlags_to_IntegrateMessaging" sourceRef="Task_FollowUpFlags" targetRef="Task_IntegrateMessaging" />
    <bpmn:sequenceFlow id="Flow_IntegrateMessaging_to_AutoNotifications" sourceRef="Task_IntegrateMessaging" targetRef="Task_AutoNotifications" />
    <bpmn:sequenceFlow id="Flow_AutoNotifications_to_RiskDelayDecision" sourceRef="Task_AutoNotifications" targetRef="Gateway_RiskDelayDecision" />
    <bpmn:sequenceFlow id="Flow_RiskDecision_Yes_to_Escalate" name="Yes" sourceRef="Gateway_RiskDelayDecision" targetRef="Task_EscalateDelays" />
    <bpmn:sequenceFlow id="Flow_RiskDecision_No_to_Merge" name="No" sourceRef="Gateway_RiskDelayDecision" targetRef="Gateway_RiskDelayMerge" />
    <bpmn:sequenceFlow id="Flow_Escalate_to_Merge" sourceRef="Task_EscalateDelays" targetRef="Gateway_RiskDelayMerge" />
    <bpmn:sequenceFlow id="Flow_Merge_to_CRMUpdate" sourceRef="Gateway_RiskDelayMerge" targetRef="Task_CRMUpdate" />
    <bpmn:sequenceFlow id="Flow_CRMUpdate_to_AttachProposal" sourceRef="Task_CRMUpdate" targetRef="Task_AttachProposalCRM" />
    <bpmn:sequenceFlow id="Flow_AttachProposal_to_GenerateReports" sourceRef="Task_AttachProposalCRM" targetRef="Task_GenerateReports" />
    <bpmn:sequenceFlow id="Flow_GenerateReports_to_End" sourceRef="Task_GenerateReports" targetRef="EndEvent_BidReady" />
  </bpmn:process>

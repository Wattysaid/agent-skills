  <bpmn:collaboration id="Collaboration_LeaveRequest">
    <bpmn:participant id="Participant_Employee" name="Employee" processRef="Process_Employee" />
    <bpmn:participant id="Participant_Manager" name="Manager" processRef="Process_Manager" />
    <bpmn:messageFlow id="MessageFlow_Request" sourceRef="Task_SubmitRequest" targetRef="StartEvent_RequestReceived" />
    <bpmn:messageFlow id="MessageFlow_Approval" sourceRef="Task_ApproveRequest" targetRef="Task_ReceiveDecision" />
    <bpmn:messageFlow id="MessageFlow_Rejection" sourceRef="Task_RejectRequest" targetRef="Task_ReceiveDecision" />
  </bpmn:collaboration>
  <bpmn:process id="Process_Employee" name="Employee Process" isExecutable="false">
    <bpmn:startEvent id="StartEvent_LeaveNeeded" name="Leave Needed">
      <bpmn:outgoing>Flow_Employee_Start_to_Submit</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_SubmitRequest" name="Submit Leave Request">
      <bpmn:incoming>Flow_Employee_Start_to_Submit</bpmn:incoming>
      <bpmn:outgoing>Flow_Employee_Submit_to_Receive</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_Employee_Start_to_Submit" sourceRef="StartEvent_LeaveNeeded" targetRef="Task_SubmitRequest" />
    <bpmn:sequenceFlow id="Flow_Employee_Receive_to_End" sourceRef="Task_ReceiveDecision" targetRef="EndEvent_Employee" />
    <bpmn:sequenceFlow id="Flow_Employee_Submit_to_Receive" sourceRef="Task_SubmitRequest" targetRef="Task_ReceiveDecision" />
    <bpmn:receiveTask id="Task_ReceiveDecision" name="Receive Decision">
      <bpmn:incoming>Flow_Employee_Submit_to_Receive</bpmn:incoming>
      <bpmn:outgoing>Flow_Employee_Receive_to_End</bpmn:outgoing>
    </bpmn:receiveTask>
    <bpmn:endEvent id="EndEvent_Employee" name="Process Ended">
      <bpmn:incoming>Flow_Employee_Receive_to_End</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
  <bpmn:process id="Process_Manager" name="Manager Process" isExecutable="false">
    <bpmn:laneSet>
      <bpmn:lane id="Lane_Manager" name="Manager">
        <bpmn:flowNodeRef>StartEvent_RequestReceived</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReviewRequest</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Approved</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SendReminder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ApproveRequest</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RejectRequest</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_UpdateSystem</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Manager</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>BoundaryEvent_Reminder</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_RequestReceived" name="Leave Request Received">
      <bpmn:outgoing>Flow_Manager_Start_to_Review</bpmn:outgoing>
      <bpmn:messageEventDefinition messageRef="Message_LeaveRequest" />
    </bpmn:startEvent>
    <bpmn:userTask id="Task_ReviewRequest" name="Review Leave Request">
      <bpmn:incoming>Flow_Manager_Start_to_Review</bpmn:incoming>
      <bpmn:outgoing>Flow_Manager_Review_to_Gateway</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_Approved" name="Request Approved?">
      <bpmn:incoming>Flow_Manager_Review_to_Gateway</bpmn:incoming>
      <bpmn:outgoing>Flow_Manager_Approved_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_Manager_Approved_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_SendReminder" name="Send Reminder to Manager">
      <bpmn:incoming>Flow_Boundary_to_Reminder</bpmn:incoming>
    </bpmn:userTask>
    <bpmn:boundaryEvent id="BoundaryEvent_Reminder" attachedToRef="Task_ReviewRequest">
      <bpmn:outgoing>Flow_Boundary_to_Reminder</bpmn:outgoing>
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration>PT2D</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:boundaryEvent>
    <bpmn:sequenceFlow id="Flow_Manager_Start_to_Review" sourceRef="StartEvent_RequestReceived" targetRef="Task_ReviewRequest" />
    <bpmn:sequenceFlow id="Flow_Manager_Review_to_Gateway" sourceRef="Task_ReviewRequest" targetRef="Gateway_Approved" />
    <bpmn:sequenceFlow id="Flow_Manager_Approved_Yes" name="Yes" sourceRef="Gateway_Approved" targetRef="Task_ApproveRequest" />
    <bpmn:sequenceFlow id="Flow_Manager_Approved_No" name="No" sourceRef="Gateway_Approved" targetRef="Task_RejectRequest" />
    <bpmn:sequenceFlow id="Flow_Manager_Approve_to_Update" sourceRef="Task_ApproveRequest" targetRef="Task_UpdateSystem" />
    <bpmn:sequenceFlow id="Flow_Manager_Reject_to_End" sourceRef="Task_RejectRequest" targetRef="EndEvent_Manager" />
    <bpmn:sequenceFlow id="Flow_Manager_Update_to_End" sourceRef="Task_UpdateSystem" targetRef="EndEvent_Manager" />
    <bpmn:sequenceFlow id="Flow_Boundary_to_Reminder" sourceRef="BoundaryEvent_Reminder" targetRef="Task_SendReminder" />
    <bpmn:userTask id="Task_ApproveRequest" name="Approve Request">
      <bpmn:incoming>Flow_Manager_Approved_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_Manager_Approve_to_Update</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_RejectRequest" name="Reject Request">
      <bpmn:incoming>Flow_Manager_Approved_No</bpmn:incoming>
      <bpmn:outgoing>Flow_Manager_Reject_to_End</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_UpdateSystem" name="Update HR System">
      <bpmn:incoming>Flow_Manager_Approve_to_Update</bpmn:incoming>
      <bpmn:outgoing>Flow_Manager_Update_to_End</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_Manager" name="Process Ended">
      <bpmn:incoming>Flow_Manager_Update_to_End</bpmn:incoming>
      <bpmn:incoming>Flow_Manager_Reject_to_End</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
